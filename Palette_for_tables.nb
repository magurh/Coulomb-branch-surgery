          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"strip", "[", "s_String", "]"}], ":=", 
           RowBox[{"StringReplace", "[", 
            RowBox[{"s", ",", 
             RowBox[{
              RowBox[{
              "RegularExpression", "[", "\"\<^\\\\s*(.*?)\\\\s*$\>\"", "]"}], 
              "\[Rule]", "\"\<$1\>\""}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"strip", "[", "e_", "]"}], ":=", "e"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
