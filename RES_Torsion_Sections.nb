Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"SectionsRES", "[", 
     RowBox[{"g2_", ",", "g3_", ",", "u_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "c0", ",", "c1", ",", "c2", ",", "d0", ",", "d1", ",", "d2", ",", "x", 
        ",", "y", ",", " ", "Coeffs", ",", " ", "Sols"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Coeffs", "=", 
        RowBox[{
         RowBox[{"CoefficientList", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"-", 
              RowBox[{"g3", "[", "u", "]"}]}], "-", 
             RowBox[{
              RowBox[{"g2", "[", "u", "]"}], " ", "x"}], "+", 
             RowBox[{"4", " ", 
              SuperscriptBox["x", "3"]}], "-", 
             SuperscriptBox["y", "2"]}], "/.", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"x", "\[Rule]", " ", 
               RowBox[{
                RowBox[{"c2", " ", 
                 SuperscriptBox["u", "2"]}], "+", 
                RowBox[{"c1", " ", "u"}], " ", "+", " ", "c0"}]}], ",", " ", 
              RowBox[{"y", "\[Rule]", "  ", 
               RowBox[{
                RowBox[{"d2", " ", 
                 SuperscriptBox["u", "2"]}], "+", 
                RowBox[{"d1", " ", "u"}], " ", "+", " ", "d0"}]}]}], "}"}]}], 
           ",", "u"}], "]"}], "//", "Simplify"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Sols", "=", 
        RowBox[{
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"Coeffs", "==", 
             RowBox[{"{", 
              RowBox[{
              "0", ",", "0", ",", "0", ",", "0", ",", "0", ",", "0", ",", 
               "0"}], "}"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "c0", ",", "c1", ",", "c2", ",", "d0", ",", "d1", ",", "d2"}], 
            "}"}]}], "]"}], "//", "Simplify"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"{", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"c2", " ", 
              SuperscriptBox["u", "2"]}], "+", 
             RowBox[{"c1", " ", "u"}], " ", "+", " ", "c0"}], ",", "  ", 
            RowBox[{
             RowBox[{"d2", " ", 
              SuperscriptBox["u", "2"]}], "+", 
             RowBox[{"d1", " ", "u"}], " ", "+", " ", "d0"}]}], "}"}], "/.", 
          RowBox[{"Sols", "[", 
           RowBox[{"[", "k", "]"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "1", ",", 
           RowBox[{"Sols", "//", "Length"}]}], "}"}]}], "]"}]}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], " ", 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"TorsionOrder", "[", 
     RowBox[{"P1_", ",", "G2_", ",", "U_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"Pnew", ",", "k", ",", " ", 
        RowBox[{"g2", "=", "G2"}], ",", " ", 
        RowBox[{"u", "=", "U"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"k", "=", "1"}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"P1", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
         RowBox[{"k", "=", "2"}]}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"k", "!=", "2"}], ",", 
         RowBox[{
          RowBox[{"Pnew", " ", "=", " ", 
           RowBox[{
            RowBox[{"DoubleP", "[", 
             RowBox[{"P1", ",", 
              RowBox[{"g2", "[", "u", "]"}]}], "]"}], "//", "Simplify"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"k", "=", "2"}], ";", "\[IndentingNewLine]", 
          RowBox[{"Do", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"Pnew", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
               RowBox[{
                RowBox[{"k", "=", 
                 RowBox[{"2", "*", "k"}]}], ";", 
                RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"k", " ", "+=", "1"}], ";", "\[IndentingNewLine]", 
             RowBox[{"Pnew", "=", 
              RowBox[{
               RowBox[{
                RowBox[{"AddP1P2", "[", 
                 RowBox[{"P1", ",", "Pnew"}], "]"}], "//", "Simplify"}], "//",
                "Quiet"}]}], ";", "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"ToString", "[", 
                  RowBox[{"Pnew", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], "]"}], "==", 
                 "\"\<ComplexInfinity\>\""}], " ", "||", 
                RowBox[{
                 RowBox[{"ToString", "[", 
                  RowBox[{"Pnew", "[", 
                   RowBox[{"[", "2", "]"}], "]"}], "]"}], "==", " ", 
                 "\"\<ComplexInfinity\>\""}], "  ", "||", 
                RowBox[{
                 RowBox[{"ToString", "[", 
                  RowBox[{"Pnew", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], "]"}], "==", 
                 "\"\<Indeterminate\>\""}], " ", "||", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"ToString", "[", 
                  RowBox[{"Pnew", "[", 
                   RowBox[{"[", "2", "]"}], "]"}], "]"}], "==", 
                 "\"\<Indeterminate\>\""}]}], " ", ",", "\[IndentingNewLine]", 
               RowBox[{"Break", "[", "]"}]}], "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", "5"}], "}"}]}], " ", 
           "\[IndentingNewLine]", "\t", "]"}]}]}], 
        RowBox[{"(*", 
         RowBox[{
         "only", " ", "check", " ", "up", " ", "to", " ", "Z5", " ", 
          "generators"}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
       "\[IndentingNewLine]", "k"}]}], "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WeirstrassSingularFibers", "[", 
   RowBox[{"g2_", ",", "g3_", ",", "t_", ",", "x_", ",", "y_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"Vars", ",", " ", "Curve", ",", " ", "Sing"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Vars", "=", 
      RowBox[{"{", 
       RowBox[{"x", ",", "y", ",", "t"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Curve", "=", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"y", "^", "2"}]}], "+", 
       RowBox[{"4", " ", 
        RowBox[{"x", "^", "3"}]}], "-", 
       RowBox[{
        RowBox[{"g2", "[", "t", "]"}], " ", "x"}], "-", 
       RowBox[{"g3", "[", "t", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Sing", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"Curve", "//", "Expand"}], ",", 
           RowBox[{"Vars", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "//", "FullSimplify"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "3"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Sing", " ", "=", " ", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"Sing", "\[Equal]", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", "0"}], "}"}]}], "}"}], ",", "Vars"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Select", "[", 
      RowBox[{"Sing", ",", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Curve", "/.", "#"}], ")"}], "==", "0"}], "&"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
