Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"AddP1P2", "[", 
     RowBox[{"P1_", ",", "P2_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x1", ",", "x2", ",", "y1", ",", "y2", ",", "\[Lambda]0"}], 
       "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"x1", "=", 
        RowBox[{"P1", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", 
       RowBox[{"y1", "=", 
        RowBox[{"P1", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"x2", "=", 
        RowBox[{"P2", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", 
       RowBox[{"y2", "=", 
        RowBox[{"P2", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"\[Lambda]0", "=", 
        RowBox[{
         RowBox[{"1", "/", "2"}], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"y2", "-", "y1"}], ")"}], "/", 
          RowBox[{"(", 
           RowBox[{"x2", "-", "x1"}], ")"}]}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "x1"}], "-", "x2", "+", 
          RowBox[{"\[Lambda]0", "^", "2"}]}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], " ", "\[Lambda]0", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "x1"}], "-", "x2", "+", 
               RowBox[{"\[Lambda]0", "^", "2"}]}], ")"}], "-", "x1"}], 
            ")"}]}], "-", "y1"}]}], "}"}]}]}], "\[IndentingNewLine]", "]"}]}],
    ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DoubleP", "[", 
    RowBox[{"P_", ",", "g2_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x1", ",", "y1", ",", "\[Lambda]0"}], "}"}], ",", 
